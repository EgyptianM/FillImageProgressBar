<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FillImageProgressBar">
        <attr name="filledSectionColor" format="color" />
        <attr name="unfilledSectionColor" format="color" />
        <attr name="fillOrientation" format="enum">
            <enum name="top_bottom" value="0" />
            <enum name="bottom_top" value="1" />
            <enum name="left_right" value="2" />
            <enum name="right_left" value="3" />
        </attr>
        <attr name="src" format="reference" />
    </declare-styleable>
</resources>